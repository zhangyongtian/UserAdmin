nxinid=this.blog.useryonghu.id;\r\n\t\t\t\t\tdeletexinxin(JSON.stringify(userandxinxin)).then(res=>{\r\n\t\t\t\t\t\tconsole.log(res)\r\n\t\t\t\t\t}).catch(error=>{\r\n\t\t\t\t\t\t\r\n\t\t\t\t\t})\r\n\t\t\t\t\tconsole.log(\"取注\")\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t},\r\n\t\tfilters:{\r\n\t\t\tdateFilter(item){\r\n\t\t\t\tlet pointFlag=item.indexOf(\".\");\r\n\t\t\t\treturn item.substring(0,pointFlag).replace(\"T\",\"  \");\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n\t.blogDetail{\r\n\t\t.blogDetail_cotent{\r\n\t\t\t.blogDetail_cotent_head{\r\n\t\t\t\tdisplay: flex;\r\n\t\t\t\t.blogDetail_cotent_head_l{\r\n\t\t\t\t\tflex: 1;\r\n\t\t\t\t\tdisplay: flex;\r\n\t\t\t\t\tjustify-content: center;\r\n\t\t\t\t\talign-items: center;\r\n\t\t\t\t\t&> img{\r\n\t\t\t\t\t\twidth: 50px;\r\n\t\t\t\t\t\theight: 50px;\r\n\t\t\t\t\t\tborder-radius: 50%;\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t\t\r\n\t\t\t\t.blogDetail_cotent_head_r{\r\n\t\t\t\t\tflex: 9;\r\n\t\t\t\t\tdisplay: flex;\r\n\t\t\t\t\tflex-direction: column;\r\n\t\t\t\t\talign-items: flex-start;\r\n\t\t\t\t\tjustify-content: center;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</style>\r\n"]}]}